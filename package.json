{
  "name": "flashkill",
  "version": "1.3.13",
  "description": "This browser extension aims to improve the user experience on for both players and spectators of the 99Damage league websites.",
  "scripts": {
    "clean:remove-files": "find dist -regextype posix-extended -regex '.*\\.(js|html|css)' | xargs --no-run-if-empty -t rm",
    "clean:remove-tree": "find dist -type d -empty -delete",
    "clean": "npm-run-all clean:*",
    "build:compile-ts": "find src -type f -name '*.ts' | xargs --no-run-if-empty -t npx tsc --target es5 --outDir dist/scripts",
    "build:copy-other": "cd src && find . -regextype posix-extended -regex '.*\\.(js|html|css)' -exec cp --parents \\{\\} ../dist \\;",
    "build": "npm-run-all build:*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flashkillapp/flashkill.git"
  },
  "author": "Timon Lapawczyk, Yannic Lapawczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flashkillapp/flashkill/issues"
  },
  "homepage": "https://github.com/flashkillapp/flashkill#readme",
  "devDependencies": {
    "@types/chrome": "0.0.114",
    "@types/node": "^14.0.6",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/jquery": "^3.3.38"
  }
}
